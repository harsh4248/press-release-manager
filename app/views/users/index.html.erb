<h1>All Press Releases</h1>
<div id="press_releases">
  <%= turbo_frame_tag "press_releases" do %>
    <% @press_releases.each do |press_release| %>
      <%= render partial: "press_releases/press_release", locals: { press_release: press_release } %>
    <% end %>
  <% end %>
</div>

<%= button_to "Sign Out", destroy_user_session_path, method: :delete%>